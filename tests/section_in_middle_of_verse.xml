	<office:body>
		<office:text text:use-soft-page-breaks="true">
			
			
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P253">Soolomong 'Yienii Gbangu</text:p>
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P3">
				<text:span text:style-name="T5">1</text:span>
				<text:span text:style-name="T4">'Yieme 'puo 'yienii ang i Soolomong 'yienii.</text:span>
			</text:p>
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P273">Suunem 'deme 'bi-poga</text:p>
			<text:p text:style-name="P185">2
				<text:tab/>
				Veng ka u mugihima aning u nuori mugihibu, </text:p>
			<text:p text:style-name="P357">'bojung i nungbu seuye waing.</text:p>
			<text:p text:style-name="P185">3
				<text:tab/>
				I kpangsuunhi 'nyuu-noo 'jung, </text:p>
			<text:p text:style-name="P357">i 'yuori 'woniye kpaang bang buo bahi. 
				<text:s/></text:p>
			<text:p text:style-name="P301">Bila 'jung ka a pogisarihi numii ni.</text:p>
			<text:p text:style-name="P185">4
				<text:tab/>
				Tarima garaning. 
				<text:s/></text:p>
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P379">Jeruusalem 'bi-pogiba</text:p>
			<text:p text:style-name="P301">Ti na jo 'tuing. 
				<text:s/></text:p>
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P379">Suunem 'bi-poga</text:p>
			<text:p text:style-name="P301">A naa tarimang wa kpening u 'debilii. 
				<text:s/></text:p>
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P379">Jeruusalem 'bi-pogiba</text:p>
			<text:p text:style-name="P301">Ti iinhi na maang ka ti nyengina i yela.</text:p>
			<text:p text:style-name="P301">Ti na tiehiye i nungbu yela a gaa waing. </text:p>
			<text:p text:style-name="P185"/>
			<text:p text:style-name="P3">
				<text:span text:style-name="T4">
					<text:tab/>
					<text:tab/>
					<text:tab/>
					<text:tab/>
				</text:span>
				<text:span text:style-name="T5">Suunem 'bi-poga</text:span>
			</text:p>
			<text:p text:style-name="P301">A taye bila ka ba numii.</text:p>
			<text:p text:style-name="P185">5
				<text:tab/>
				Jeruuselem 'bi-pogiba 'woi, </text:p>
			<text:p text:style-name="P357">n iye sagilaa che viela a'wo 'Kii-der tentihi, </text:p>
			<text:p text:style-name="P357">a'wo Soolomong patiisahi.</text:p>

		</office:text>
	</office:body>
